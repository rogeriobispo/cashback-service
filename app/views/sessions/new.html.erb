<h1>Login</h1>
<%= form_with  url: sessions_url, class: "nifty_form" do |f| %>
  <%= f.label :email, 'Email'%>
  <%= f.email_field :email, class: 'form-control'%>
  <%= f.label :password, 'Senha'%>
  <%= f.password_field :password, class: 'form-control'%>
  <br>
  <%= f.submit "Enviar", class: "btn btn-primary" %>
<% end %>
